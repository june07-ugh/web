{
  "name": "ugh-web",
  "version": "0.1.0",
  "scripts": {
    "dev": "vite --host 0.0.0.0",
    "dev:extension": "npm-run-all --parallel build:dev watch:rollup:deps",
    "build:dev": "vite build --mode development -w --base /dist/ --sourcemap --minify false",
    "build": "npm-run-all --serial build:ui",
    "build:ui": "cross-env NODE_ENV=production && vite build --minify true",
    "build:sw": "node utils/build-sw.js",
    "build:deps": "cross-env NODE_ENV=production node utils/watch-rollup.js",
    "watch:rollup:deps": "node utils/watch-rollup.js",
    "preview": "vite preview",
    "test": "playwright test --project \"edge@latest@localhost\" \"chrome@latest@localhost\" --workers 5 --retries 3",
    "test:local": "playwright test --project \"edge@latest@localhost\" \"chrome@latest@localhost\" --workers 1",
    "test:post": "playwright test --project \"edge@latest@localhost\" \"chrome@latest@localhost\" --workers 5 --retries 3",
    "rimraf": "rimraf dist",
    "zip": "node utils/update-manifest-and-zip.js"
  },
  "dependencies": {
    "@material-design-icons/font": "^0.14.13",
    "animate.css": "^4.1.1",
    "async": "^3.2.4",
    "axios": "^1.6.8",
    "bootstrap": "^5.3.3",
    "cookie": "^0.6.0",
    "cropperjs": "^1.6.2",
    "dropzone": "^6.0.0-beta.2",
    "keycloak-js": "^24.0.3",
    "nanoid": "^5.0.4",
    "pinia": "^2.1.7",
    "pinia-plugin-persistedstate": "^3.2.1",
    "sweetalert2": "^11.6.13",
    "tui-image-editor": "^3.15.3",
    "uuid": "^9.0.1",
    "vite": "^5.2.8",
    "vite-plugin-pwa": "^0.20.0",
    "vite-plugin-vuetify": "^2.0.3",
    "vue": "^3.4.23",
    "vue-router": "^4.3.2",
    "vue-social-sharing": "^4.0.0-alpha4",
    "vuetify": "^3.5.17"
  },
  "devDependencies": {
    "@playwright/test": "^1.29.1",
    "@vitejs/plugin-vue": "^5.0.3",
    "archiver": "^7.0.1",
    "browserstack-local": "^1.5.1",
    "chokidar": "^3.5.3",
    "clean-css": "^5.3.3",
    "cross-env": "^7.0.3",
    "eslint": "^8.31.0",
    "npm-run-all": "^4.1.5",
    "playwright": "^1.29.1",
    "rollup-plugin-copy": "^3.5.0",
    "rollup-plugin-terser": "^7.0.2",
    "sass": "^1.56.2",
    "terser": "^5.30.4",
    "vite-plugin-compression": "^0.5.1",
    "vue-eslint-parser": "^9.1.0"
  }
}
